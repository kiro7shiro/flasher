<div class="w3-container" id="workspaceMenu">
    <div class="w3-bar w3-black w3-center w3-small">
        <div class="w3-dropdown-hover">
            <a href="#" class="w3-bar-item w3-button">ADD VISUALIZER</a>
            <div class="w3-dropdown-content w3-bar-block w3-card-4">
                <% visualizers.forEach(visualizer=> { %>
                    <a href="#" class="w3-bar-item w3-button" data-action="add-visualizer" data-type="<%= visualizer %>">
                        <%= visualizer.toUpperCase() %>
                    </a>
                    <% })%>
            </div>
        </div>
        <a href="#" data-action="del-visualizer" class="w3-bar-item w3-button">DEL VISUALIZER</a>
        <div class="w3-dropdown-hover">
            <a href="#" class="w3-bar-item w3-button">ADD NODE</a>
            <div class="w3-dropdown-content w3-bar-block w3-card-4">
                <% nodes.forEach(node=> { %>
                    <a href="#" class="w3-bar-item w3-button" data-action="add-node" data-type="<%= node %>">
                        <%= node.toUpperCase() %>
                    </a>
                    <% })%>
            </div>
        </div>
        <a href="#" data-action="del-node" class="w3-bar-item w3-button">DEL NODE</a>
    </div>
</div>