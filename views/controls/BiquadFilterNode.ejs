<div class="w3-row" id="<%= id %>">
    <h6><%= id %></h6>
    <div class="w3-quarter w3-center">
        <%- include('Fader', { title: 'freq', id: 'frequency', min: '20', max: '22000', value: '0'}) %>
    </div>
    <div class="w3-quarter w3-center">
        <div>detune</div>
        <input type="range" orient="vertical" min="0" max="1" step=".05" value="0" id="detune">
    </div>
    <div class="w3-quarter w3-center">
        <div>Q</div>
        <input class="w3-input" type="number" name="Q" id="Q" value="=0">
    </div>
    <div class="w3-quarter w3-center">
        <div>gain</div>
        <input type="range" orient="vertical" min="-40" max="40" value="0" id="gain">
    </div>
</div>