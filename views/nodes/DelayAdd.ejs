<div>
    <% if (locals.id===null || locals.id===undefined) locals.id='delay-add' %>
    <div id="<%= id %>" class="w3-row">
        <div class="w3-padding">
            delay add
        </div>
        <div class="w3-row w3-padding">
            <div class="w3-col" style="width:60px">&nbsp;</div>
            <div class="w3-rest">
                <%- await include('Slider', { id: 'delay' , min: '0' , max: '1' , value: '0', step: '0.01' ,
                    action:'set-delay' }) %>
            </div>
            <div class="w3-col" style="width:60px">&nbsp;</div>
            <div class="w3-rest">
                <%- await include('Slider', { id: 'gain' , min: '-100' , max: '100' , value: '0', step: '1' ,
                    action:'set-gain' }) %>
            </div>
        </div>
    </div>
</div>