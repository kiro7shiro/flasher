<li>
    <div id="analyser" class="w3-row">
        <div class="w3-padding">analyzer</div>
        <div class="w3-row w3-padding">
            <div class="w3-col" style="width:60px">min DB</div>
            <div class="w3-rest">
                <%- await include('Slider', {id: 'minDecibels' , action:'set-analyser', detail: 'minDecibels' , min: -100,
                    max: 0, value: 0 }) %>
            </div>
        </div>
        <div class="w3-row w3-padding">
            <div class="w3-col" style="width:60px">max DB</div>
            <div class="w3-rest">
                <%- await include('Slider', {id: 'maxDecibels' , action:'set-analyser', detail: 'maxDecibels' , min: -100,
                    max: 0, value: 0 }) %>
            </div>
        </div>
        <div class="w3-row w3-padding">
            <div class="w3-col" style="width:60px">STC</div>
            <div class="w3-rest">
                <%- await include('Slider', {id: 'smoothingTimeConstant' , action:'set-analyser',
                    detail: 'smoothingTimeConstant' , min: 0, max: 1, step: 0.01, value: 0 }) %>
            </div>
        </div>
    </div>
</li>
