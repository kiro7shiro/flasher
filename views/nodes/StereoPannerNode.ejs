<div>
    <% if (locals.id===null || locals.id===undefined) locals.id='panner' %>
    <div id="<%= id %>" class="w3-row">
        <div class="w3-padding">
            pan
        </div>
        <div class="w3-row w3-padding">
            <div class="w3-col" style="width:60px">&nbsp;</div>
            <div class="w3-rest">
                <%- await include('Slider', { id: 'pan' , min: '-1' , max: '1' , value: '0' , step: '0.01' ,
                    action:'set-panner' }) %>
            </div>
        </div>
    </div>
</div>